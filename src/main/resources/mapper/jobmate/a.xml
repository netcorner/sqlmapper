<?xml version="1.0" encoding="utf-8"?>
<!--DOCTYPE sqlMap SYSTEM "https://raw.githubusercontent.com/netcorner/sqlmapper/master/files/sqlMap.dtd"-->
<sqlMap>
	<function>
		#macro(getTable $t)

		select *from $t

		#end

		#macro(getTable2 $t)

		select *from $t

		#end
		#macro(appendFun)

		 and 1=2

		#end
	</function>


	<statement id="add">
		<config>
		set names utf8mb4;
		</config>
		<insert>
			#Insert()
		</insert>
	</statement>
	<statement id="edit">
		<update>
			#Update()
		</update>
	</statement>
	<statement id="del">
		<delete>
			#Delete()
		</delete>
	</statement>
	<statement id="finder">
		<select id="t1">
			select*from $table where #Where() order by $primary desc
		</select>
	</statement>
	<statement id="page">
		<page></page>
	</statement>


	<statement id="fun2">
		<select>
			#getTable2('a')
		</select>
	</statement>
	<statement id="AfterExecId">
		<select afterExecId="fun2">
			select*from $table where 1=1
		</select>
	</statement>
	<statement id="BeforeExecId">
		<select beforeExecId="fun2">
			select*from $table where 1=1
		</select>
	</statement>
	<statement id="ExecAppendSql">
		<select execAppendSql="#appendFun()">
			select*from $table where 1=1
		</select>
	</statement>
</sqlMap>