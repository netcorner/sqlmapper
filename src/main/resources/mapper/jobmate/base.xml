<?xml version="1.0" encoding="utf-8"?>
<!--DOCTYPE sqlMap SYSTEM "https://raw.githubusercontent.com/netcorner/sqlmapper/master/files/sqlMap.dtd"-->
<sqlMap>
	<function>
		##得到最大 id
		#macro(getMaxID)
		select max(id) id from $table
		#end
	</function>
	<method>
		<sql id="a">
			insert a
			#Insert()
		</sql>
	</method>
	<statement id="_insert" filter="true">
		<insert>
			#Insert()
		</insert>
	</statement>
	<statement id="_update">
		<update>
			#Update()
		</update>
	</statement>
	<statement id="_delete">
		<delete>
			#Delete()
		</delete>
	</statement>
	<statement id="_list">
		<select>
			select*from $table where #Where()
		</select>
	</statement>
	<statement id="_page">
		<page></page>
	</statement>
	<statement id="add">
		<insert>
			#Insert()
		</insert>
	</statement>
	<statement id="edit">
		<update>
			#Update()
		</update>
	</statement>
	<statement id="del">
		<delete>
			#Delete()
		</delete>
	</statement>
	<statement id="finder">
		<select>
			select*from $table where #Where()
		</select>
	</statement>
	<statement id="page">
		<page></page>
	</statement>

</sqlMap>